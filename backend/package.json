  "scripts": {
    "build": "nest build",
    "console:dev": "ts-node -r tsconfig-paths/register src/library/utilities/console.ts",
    "console": "node dist/src/library/utilities/console.js",
    "createSchema": "node_modules/.bin/ts-node -r tsconfig-paths/register src/library/utilities/create-schema.ts ",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "generateDocs": "node_modules/.bin/spectaql graphqlDocs/config.yml -t ./graphqlDocs/docs",
    "import:dev": "npm run console:dev import-entities",
    "import": "node dist/src/console.js import-entities",
    "import-event-subscribers:dev": "npm run console:dev import-event-subscribers",
    "import-event-subscribers": "npm run console import-event-subscribers",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "migration:local:config": "node_modules/.bin/ts-node -r tsconfig-paths/register src/library/utilities/migration local",
    "migration:local:generate": "npm run typeorm:local -- migration:generate -n ",
    "migration:local:create": "npm run typeorm:local -- migration:create -n ",
    "migration:local:revert": "npm run typeorm:local -- migration:revert",
    "migration:local:run": "npm run typeorm:local -- migration:run",
    "migration:local:show": "npm run typeorm:local -- migration:show",
    "prebuild": "rimraf dist",
    "test": "node_modules/.bin/ts-node -r tsconfig-paths/register ./test/pre-tests && node_modules/.bin/jest --ci --testTimeout=20000 --runInBand --coverage --passWithNoTests --detectOpenHandles --forceExit --config ./jest.config.js",
    "sniff": "eslint src/**/*.ts --fix -f json -o ../backend-lint-report.json",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/src/main.js",
    "start": "nest start",
    "typeorm:local": "node_modules/.bin/ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "postinstall": "npm run prestart:prod",
